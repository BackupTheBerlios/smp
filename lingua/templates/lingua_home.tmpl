      <td colspan="2">
        <TMPL_IF PAGE_CAT_START_LINK>
          <a class="normal" href="<TMPL_VAR PAGE_CAT_START_LINK>"><TMPL_VAR PAGE_CAT_START_NAME></a>
        <TMPL_ELSE>
          <span class="normal">
            <TMPL_VAR PAGE_CAT_START_NAME>
          </span>
        </TMPL_IF>

        <TMPL_LOOP PAGE_CAT_LIST>
          <span class="normal">-></span>

          <TMPL_IF PAGE_CAT_LIST_LINK>
            <a class="normal" href="<TMPL_VAR PAGE_CAT_LIST_LINK>"><TMPL_VAR PAGE_CAT_LIST_NAME></a>
